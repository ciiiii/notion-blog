{
  "name": "notion-blog",
  "version": "0.1.0",
  "private": true,
  "description": "Blog based on Notion.",
  "author": "ciiiii <allen.cccccnm@gmail.com>",
  "repository": "ciiiii/notion-blog",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "vercel --prod",
    "deps": "run-s deps:*",
    "deps:update": "[ -z $GITHUB_ACTIONS ] && yarn add notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link": "[ -z $GITHUB_ACTIONS ] && yarn link notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:link on CI'",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "test": "run-s test:*",
    "test:lint": "eslint .",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check",
    "posttest": "run-s build"
  },
  "dependencies": {
    "@google-cloud/firestore": "^4.10.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.25.0",
    "expiry-map": "^2.0.0",
    "fathom-client": "^3.0.0",
    "got": "^11.8.2",
    "graphemer": "^1.3.0",
    "isomorphic-unfetch": "^3.1.0",
    "keyv": "^4.1.1",
    "lqip-modern": "^1.2.0",
    "moment": "^2.29.1",
    "next": "^12.1.0",
    "node-fetch": "^2.6.1",
    "notion-client": "^6.0.6",
    "notion-types": "^6.0.6",
    "notion-utils": "^6.0.6",
    "p-map": "^5.3.0",
    "p-memoize": "^6.0.1",
    "react": "^17.0.2",
    "react-body-classname": "^1.3.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-notion-x": "^6.0.6",
    "react-static-tweets": "^0.7.1",
    "react-use": "^17.3.2",
    "static-tweets": "^0.7.1",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^11.1.2",
    "@types/classnames": "^2.3.1",
    "@types/node": "^16.11.2",
    "@types/node-fetch": "^3.0.3",
    "@types/react": "^17.0.31",
    "@types/react-body-classname": "^1.1.7",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "autoprefixer": "^10.4.4",
    "cross-env": "^7.0.2",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.26.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "prettier": "^2.4.1",
    "sass": "^1.49.9",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.4.4"
  }
}
